{
  "name": "desafio",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev:front": "parcel serve ./client/index.html",
    "dev:api": "nodemon --watch ./server/index.ts --exec ts-node ./server/dev.ts",
    "build:front": "parcel build ./client/index.html",
    "buil:api": "tsc ./server/index.ts ./server/index.js",
    "build": "yarn build:front && yarn build:api",
    "start": "node ./server/index.js"
  },
  "dependencies": {
    "@types/node": "^16.11.10",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "firebase-admin": "^10.0.0",
    "nodemon": "^2.0.15",
    "parcel": "^2.0.0-rc.0",
    "typescript": "^4.5.2"
  }
}
